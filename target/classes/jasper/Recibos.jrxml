	<queryString>
		<![CDATA[SELECT empresas.razonsocial,
	empresas.direccion,
	empresas.email,
	empresas.telefono,
	facturas.id,
	CONCAT( clientes.nombres,"",clientes.apellidos) AS nombres,
	pagos.fecha_pago,
	pagos.pago,
	pagos.saldo
FROM pagos,
	clientes,
	facturas,
	empresas
