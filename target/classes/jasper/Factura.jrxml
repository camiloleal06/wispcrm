	<queryString language="SQL">
		<![CDATA[SELECT
clientes.identificacion,
clientes.nombres,
clientes.apellidos,
clientes.email,
clientes.telefono,
clientes.direccion,
planes.nombre,
planes.precio AS PRECIO_PLAN ,
facturas.id AS CODIGO_FACTURA,
facturas.fechapago,
facturas.fechavencimiento
FROM
facturas,
clientes,
planes
WHERE
clientes.id=facturas.cliente_id
AND
facturas.id= $P{factura_id}]]>
	</queryString>
